REG=Style:b,text:TeamViewer ID,value:HKEY_LOCAL_MACHINE\SOFTWARE\WOW6432Node\TeamViewer\ClientID,display:%1
#Informações da Maquina
DOMAIN=text:Domínio
HOST=text:Nome da Maquina
USER=text:Usuário
#Links Uteis
COLOR=%Silver%
COMMENT=color:%White%,style:iwb,text:Links Úteis
CONTROL=type:1,display:Sistema de Chamados TopDesk,uri:https://rioquente.topdesk.net/
CONTROL=type:1,display:Intranet Aviva,uri:https://grq.sharepoint.com/sites/intranet
CONTROL=type:1,display:Sistemas Aviva,uri:https://sistemas.aviva.com.br
CONTROL=type:1,display:Sistemas Sauipe,uri:https://sistemassauipe.aviva.com.br
#Informações de Desempenho
COMMENT=color:%White%,style:iwb,text:Desempenho do Sistema
#Memoria
COMMENT=text:,font-size:50%
PHYSICALRAM=interval:30,font-size:120%,threshold1:3 80 0000FF,display:%1[3.1b]B / %2[3.1b]B (%3% used)
TOPPROCESSMEM=interval:10,font-size:80%,text:Top Memory,display:%1 (pid:%2) %3[1.1b]B
PAGEFAULTS=interval:10,font-size:100%,threshold1:5 -80 2222bb,display:Tot: %1\, Hrd: %2\, Hit: %5%
#Discos
COMMENT=text:,font-size:50%
LOGICALDRIVES=interval:5,diskio:1,font-size:85%,chart:bar linear 100 7 75,threshold1:11 10000000 0000f0,threshold2:13 10000000,threshold3:7 90 0070f0, 0060f0,row-text:Drive %1: (%2)| %3,display:%6[1.0B]B/%5[1.0B]B (%7[1.1f]% used)|%chart%| r: %11[1.1B]B/s\, w: %13[1.1B]B/s\, q: %17\, i: %19%| avg read: %15[1.3B]secs\, write: %16[1.3B]secs
#CPU
WMI=interval:5,font-size:80%,text:CPU Info,namespace:root\cimv2,query:Win32_Processor,display:Cores:%NumberOfCores%|Speed:%MaxClockSpeed%MHz   Current:%CurrentClockSpeed%MHz
WMI=interval:5,text:Cpu Temp,namespace:root\wmi,query:MSAcpi_ThermalZoneTemperature,display: {{1.1f:%CurrentTemperature%/10-273.15}}C
CPU=interval:3,background-interval:10,chart:line linear 100 1 00ff00 2 0000bb,threshold1:1 80 0000ff,threshold2:2 40 0000ff,display:Tot: %1[2.0f]%\, Krnl: %2[2.0f]%\, Queue: %3
CPUUSAGE=interval:3,font-size:8,row-text:Cpu %1,chart:bar linear 100 2 80,bar-colors:%Blue% %Orange% %Magenta%,display:%2[2.0d]%
PROCESSCOUNT=interval:5,display:%1 processes\, %2 threads
TOPPROCESSCPU=interval:10,maxrows:5,text:Top Processes,display:%1 (pid:%2) %3%
COMMENT=text:,font-size:50%
CONTROL=type:1,style:ubi,text:,display:Performance Monitor,uri:perfmon

